<div class="col-sm-12">
    <h1>Loop it!</h1>
    <ul>
        <% for (var i = 0; i < length; i++) { %>
            <li><%= "LOOP " + i %></li>
        <%}%>
    </ul>
    <% if(username) { %>
        <H2>Welcome!! <%= username %> (name: <%= name %>)</H2>
    <% } else { %>
        <H2>Please Login.</H2>
    <% } %>
</div>